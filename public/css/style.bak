#contact {
	padding-bottom: 160px;
}

.footer__cate__name {
  font-size: 22px;
  text-transform: uppercase;
  padding-bottom: 45px;
  color: #1e1e1e;
  font-weight: bold;
}

.footer__link {
  font-size: 18px;
  color: #2c2c22;
  text-transform: capitalize;
  margin-bottom: 35px;
  display: inline-block;
  font-weight: lighter;
}

#not-found {
	text-align: center;
}

#not-found h2{
	font-size: 140px;
font-weight: bold;
color: #1e1e1e;
text-transform: uppercase;
margin-bottom: 52px;
}
#not-found p{
	font-size: 38px;
color: #1e1e1e;
margin-bottom: 40px;
}
#not-found a{
	width: 306px;
height: 70px;
display: inline-block;
background: #e20b50;
border-radius: 35px;
color: #fff;
