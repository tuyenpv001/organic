
@import url('https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700;900&display=swap');

*{
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}

:root {
	--color-primary: #fff969;
}

html {
	font-size: 62.5%;
}

body {
	font-size: 1.4rem;
	font-family: 'Roboto', sans-serif;
	line-height: 1.4;
	letter-spacing: .5;

}
ul {
	list-style: none;
	padding: 0;
}
li{
	list-style-type: none;
}
a {
	text-decoration: none;

}
input, textarea {
	border: none;
}

label {
  font-size: 20px;
  text-transform: capitalize;
  color: #4b4141;
  margin-bottom: 10px;
}
.pl {
  padding-left: 50px;
}
.mb-5 {
  margin-bottom: 30px;
}
.mt-main {
	margin-top: 100px;
}
.mb-140 {
  margin-bottom: 140px;
}
.mt-130 {
	margin-top: 130px;
}
.pt-130 {
	padding-top: 130px;
}
.mt-60 {
	margin-top: 60px;
}
.mt-30 {
	margin-top: 30px;
}
.mt-40 {
	margin-top: 40px;
}
.mt-20 {
	margin-top: 20px;
}
.bg-primary {
	background: var(--color-primary) !important;
}
.error_mess {
	color: red;
	padding: 10px 0;
	font-size: 14px;
	font-style: italic;
}
.error_mess:first-letter {
	text-transform: uppercase;
}
.form__subtext {
	font-size: 14px;
	margin-top: 10px;
}
.btn-order {
  width: 100%;
}

.fom__subtext a{
	color: ##e20b50;
}
/**
	Style BUTTON
*/

.btn {
  background: #e20b50;
  border-radius: 5px;
  font-size: 14px;
  text-transform: uppercase;
  display: inline-block;
  text-align: center;
  transition: background .3s ease;
  border: none;
  padding: 10px 50px;
}

span.arrow  {
  font-size: 18px;
  font-weight: bold;
  margin-left: 5px;
  transition: margin .2s ease-out;
}


.btn-banner {
	width: 226px;
}
 .btn-view {
 	width: 258px;
 }
.btn-cart {
	width: 235px;
}
.btn-login {
  width: 100%;
  color: #fff;
}
.btn:hover {
	background: rgba(226,11,80,.8);
	color: #fff;
}

.btn:hover span.arrow {
  margin-left: 10px;
}


.header--top {
  height: 45px;
  background: #000;
  color: #fff;
  display: flex;
  justify-content: space-between;
  max-width: 100%;
  padding: 0 140px;
  align-items: center;
}

.nav{
	max-width: 100%;
  padding: 0 140px;
  height: 170px;
  display: flex;
justify-content: space-between;
align-items: center;
}

.nav__list {
  display: flex;
  align-items: center;
}
.nav__item:not(:last-child) {
  margin-right: 50px;
}
.nav__link {
  font-size: 14px;
  text-transform: uppercase;
  color: #1e1e1e;
  font-weight: bold;
  display: block;
  position: relative;

}

.nav__link::after {
	content: '';
	width: 0;
	height: 10px;
	color: #fff969;
	bottom: 0;
	left: -5px;
	display: block;
	position: absolute;
	z-index: -1;
	background: #fff969;
	transition: width .3s ease;
}
.nav__link:hover {
	color: #000;
}

.nav__link:hover::after {
	width: 100%;
}
.nav__link span {
	margin-left: 8px;
}
.nav__icon {
  width: 35px;
  height: 35px;
  display: inline-block;
  text-align: center;
  line-height: 35px;
  border-radius: 50%;
  color: #fff;
}
.nav__icon:hover {
	color: #fff;
}
.nav__icon:first-child {
	background: #365596;
}
.nav__icon:nth-child(2) {
	background: #3eacdf;
}
.nav__icon:last-child {
	background: #2a445e;
}
.header--top ul {
  display: flex;
  justify-content: start;
  align-items: center;
  height: 100%;
  margin: 0;
}

.header--top ul li:not(:last-child) {
  margin-right: 50px;
}

.header--top ul li a{
	font-size: 12px;
	text-transform: uppercase;
	color: #fff;
	font-weight: bold;
}

#section__hero {
  height: 605px;
  max-width: 100%;
  padding: 0 140px;
  background-image: url('https://res.cloudinary.com/dcdjjysyu/image/upload/v1668005673/organic/banner_zrbxq2.jpg');
  background-repeat: no-repeat;
  background-size: contain;
}
.banner {
  padding-top: 170px;

}

.banner__headers::first-letter {
  text-transform: uppercase;
}
.banner__headers {
  font-size: 65px;
  text-align: center;
  margin-bottom: 38px;
  color: #fff;
}
.banner__content {
  width: 605px;
  text-align: center;
}

.category__container {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 95px 0;
}
.category {
  margin: 0 100px;
  width: 150px;
  text-align: center;
}
.category__icon {
  position: relative;
}

.category__icon::after {
  content: '';
  width: 125px;
  height: 125px;
  background: #fff969;
  display: inline-block;
  border-radius: 50%;
  position: absolute;
  top: -10%;
  left: -15%;
  z-index: -1;
}

.category__text {
  font-size: 16px;
  text-transform: uppercase;
  letter-spacing: 1px;
  font-weight: 600;
  margin-top: 40px;
}


.product {
  width: 387px;
  position: relative;
  text-align: center;
}

.product:hover .product__overlay {
	opacity: 1;
	overflow: visible;
}
.product__overlay {
  position: absolute;
  width: 100%;
  height: 430px;
  display: flex;
  justify-content: center;
  align-items: center;
  background: rgba(0,0,0,.5);
  overflow: hidden;
	opacity: 0;
	transition: all .5s ease;
}
.product__overlay a {
  width: 70px;
  height: 70px;
  display: inline-block;
  border-radius: 50%;
  color: #fff;
  background: #e20b50;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 10px;
  font-size: 20px;
  transition: background .3s ease;
}
.product__overlay a:hover {
	background: #fff969;
	color: #e20b50;
}
.product__img {
  width: 100%;
  height: 430px;
}

.product__img img {
  width: 100%;
}
.product__content {
  margin-top: 32px;
  display: block;
  cursor: pointer;
}
.product__name {
  font-size: 24px;
  text-transform: capitalize;
  color: #1e1e1e;
}
.product__price {
  font-size: 20px;
  color: #878787;
}

.title {
	height: 144px;
}
.title--main {
	font-size: 55px;
	text-transform: capitalize;
	font-weight: 500;
}

.path {
	font-size: 18px;
	text-transform: capitalize;
	font-weight: 400;
}

.about__text{
	font-size: 30px;
	text-transform: capitalize;
	font-weight: 500;
	letter-spacing: 1px;
}
.about__sub__text {


text-transform: capitalize;
font-style: italic;
font-weight: lighter;
letter-spacing: 2px;

}

.card {
	width: 260px;
	border: none !important;
}
.card__img {
	width: 100%;
  /* height: 300px; */
}
.card__img img {
	width: 100%;
height: 100%;
object-fit: cover;
} 
.card__detail {
	margin-top: 15px;
}
.card__title {
	text-transform: capitalize;
  font-size: 20px;
  letter-spacing: 1px;
  color: #414141;
}
.card__title:hover {
  color: #e20b50;
}
.card__text {
	font-size: 17px;
	font-weight: lighter;
}

.about__detail {
	margin-top: 160px;
	height: 180px;
	background: #e20b50;
	display: flex;
	align-items: center;
}

.about__card {
	color: #fff;
	height: 100%;
	width: fit-content;
	text-align: center;
}
.about__numberic {
	font-size: 40px;
font-weight: bold;
letter-spacing: 1px;
}
.about__description {
	font-size: 18px;
text-transform: capitalize;
font-weight: 400;
opacity: .95;
}

.about__detail  .row {
	width: 100%;
	padding: 0 40px;
}

.about__card > span {
	display: inline-block;
}

.input{
	width: 420px;
	height: 60px;
	padding-left: 30px;
	font-size: 18px;
	background: #f3f7fe;
	color: #878787;
}

#contact .btn {height: 70px;
width: 167px;
border-radius: 50px;
background: #e20b50;
color: #fefefe;
font-size: 14px;
