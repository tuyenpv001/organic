line-height: 70px;
}



.table__cart {
	width: 100%;
}
td, th {
	padding: 30px 0;
	vertical-align: top;
}
tr th:first-child,
tr td:first-child {
	width: 50%;
}

tr th:nth-child(2),
tr td:nth-child(2) {
	width: 16.666%;
}

tr th:nth-child(3),
tr td:nth-child(3) {
	width: 16.666%;
	text-align: right;
}
tr th:last-child,
tr td:last-child {
	width: 16.666%;
	text-align: right;
}

.cart_product {
	display: flex;
}
.cart_product img{
	margin-right: 30px;
}

.form__register {
	max-width: 60%;
}

.product__name--main {
  font-size: 55px;
  font-weight: bold;
  text-transform: capitalize;
  margin-bottom: 0;
  padding: 0;
  color: #1e1e1e;
}

.product__path {
  display: block;
  text-align: center;
  font-size: 18px;
  text-transform: uppercase;
  color: #787878;
}

.product__img__slide {
  width: 570px;
  height: 715px;
  margin-right: 128px;
}
.img__thumb {
 
  width: 570px;
  overflow: hidden;
}

.img__thumb img {
  width: 100%;
  object-fit: cover;
}

.img__small {
  margin-top: 20px;
  height: 135px;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.img__small img {
  width: 135px;

}

.product__description .product__price {
  font-size: 30px;
  color: #878787;
  letter-spacing: 1px;
  font-weight: bold;
}

.product_rate {
  font-size: 18px;
  margin-top: 35px;
margin-bottom: 45px;
}

.rate_start {
  color: #fcbc24;
  margin-right: 12px;
}

.rate_count {
  color: #878787;
  text-transform: lowercase;
}

.product_sub_description {
  font-size: 18px;
  line-height: 2;
  color: #1e1e1e;
  width: 420px;
}
.add__to__cart {
  margin-top: 40px;
  margin-bottom: 45px;
}
.quantity {
  height: 65px;
  width: 159px;
  border: 1px solid;
  padding: 25px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 18px;
  color: #1e1e1e;
  margin-right: 20px;
}



.product__category span {
  color: #8f8f8f;
  font-weight: lighter;
}
.product__category {
  font-size: 18px;
  text-transform: capitalize;
  font-weight: 600;
}

.btn__tab {
  font-size: 26px;
  text-transform: capitalize;
  font-weight: 600;
  background: none;
  border: none;
  margin: 0 17.5px;
  color: #b9b9b9;;
}
.tab__btn {
  margin-bottom: 65px;
  text-align: center;
}
.tab__container {
  width: 80%;
}
.tab__content__container {
  padding: 0 50px;
}

.tab__active {
	color: #e20b50;
  padding-bottom: 5px;
  border-bottom: 5px solid #fff869;
}

.tab__content{
